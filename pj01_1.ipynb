      "from math import sqrt\r\n",
      "def is_prime(n):\r\n",
      "    if n==1:\r\n",
      "        return False\r\n",
      "    for i in range(2,int(sqrt(n))+1):\r\n",
      "        if n%i == 0:\r\n",
      "            return False\r\n",
      "    return True\r\n",
      "\r\n",
      "if __name__ =='__main__':\r\n",
      "    n=1000000\r\n",
      "    s=0\r\n",
      "    for i in range(1000,n+1):\r\n",
      "        if is_prime(i)*is_prime(i+2):\r\n",
      "            s= s + 1\r\n",
      "    print('The total number of twim primes between 1 thousand and 1 million is %d',s)"
     ]
    }
   ],

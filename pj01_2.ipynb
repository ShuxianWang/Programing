      "from math import sqrt\r\n",
      "import sys\r\n",
      "def is_prime(n):\r\n",
      "    if n==1:\r\n",
      "        return False\r\n",
      "    for i in range(2,int(sqrt(n))+1):\r\n",
      "        if n%i == 0:\r\n",
      "            return False\r\n",
      "    return True\r\n",
      "\r\n",
      "if __name__ =='__main__':\r\n",
      "    i=sys.maxsize\r\n",
      "    while i>2:\r\n",
      "        if is_prime(i-2)*is_prime(i):\r\n",
      "            print(i,i-2)\r\n",
      "            break\r\n",
      "        i=i-1"
     ]
    }
